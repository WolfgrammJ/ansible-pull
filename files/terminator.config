[global_config]
[keybindings]
[profiles]
  [[default]]
    visible_bell = True
    background_color = "#241f31"
    background_darkness = 0.85
    background_type = transparent
    font = MesloLGS NF 10
    foreground_color = "#ffffff"
    scrollback_lines = 5000
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
      size = 1200, 800
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
